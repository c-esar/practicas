	<queryString>
		<![CDATA[SELECT
       ho.seq_historia
      ,ho.acepto_condiciones
      ,ho.desp_motivo_consulta
      ,ho.dia_historia
      ,ho.otro_evaluacion
      ,ho.recom_habitos
      ,ho.recom_medicas
      ,ho.recom_ocupacionales
      ,ho.revision_sistemas
      ,ho.seq_ciudad
      ,ho.seq_exa_fisico
      ,ho.seq_historia_laboral
      ,ho.seq_persona
      ,ho.seq_tipo_historia
      ,ho.seq_eval
      ,p.seq_persona
      ,p.afp_persona
      ,p.arl
      ,p.barrio
      ,p.celular
      ,p.codigo
      ,p.correo
      ,p.direccion
      ,p.edad
      ,p.escolaridad
      ,p.estado_civil
      ,p.fecha_creacion
      ,p.fecha_nacimiento
      ,p.genero
      ,p.grupo_sanguineo
      ,p.imagen
      ,p.nom_cargo_dep
      ,p.nom_emergencia
      ,p.nom_primer_apellido
      ,p.nom_primer_nombre
      ,p.nom_segundo_apellido
      ,p.nom_segundo_nombre
      ,p.nuevorol_usuario
      ,p.numero_documento
      ,p.parentesco_emergencia
      ,p.rh
      ,p.tel_emergencia
      ,p.telefono
      ,p.seq_aseguradora
      ,p.seq_localidad
      ,p.seq_lugar_residencia
      ,p.seq_lugar_nacimiento
      ,p.seq_perfil
      ,p.seq_tipo_documento
      ,exa.seq_exa_fisico
      ,exa.desp_adb
      ,exa.desp_cap_cuello
      ,exa.desp_column
      ,exa.desp_genitourinario
      ,exa.desp_miembros
      ,exa.desp_neurologico
      ,exa.desp_osteomuscular
      ,exa.desp_piel_fan
      ,exa.desp_torax
      ,exa.lateralidad
      ,exa.num_frec_car
      ,exa.num_frec_resp
      ,exa.num_imc
      ,exa.num_peso
      ,exa.num_sat
      ,exa.num_talla
      ,exa.num_tension
      ,lb.acciones_trabajo_sn
      ,lb.act_economica
      ,lb.antiguedad
      ,lb.cargo
      ,lb.desp_funciones_cargo
      ,lb.enfermedad_laboral_sn
      ,dat_fecha_ingreso
      ,nom_empresa
      ,s.nom_aseguradora
      ,tipo.sigla_documento
      ,ciuR.nom_ciudad as cuidad_residencia
      ,ciuN.nom_ciudad as ciudad_nacimiento
      ,ciuO.nom_ciudad as ciudad_ocupacional
      ,ciuO.cod_dpto
      ,(SELECT IIF (EXISTS (select 1 from hc_historia_ocupacional where seq_eval = 1 and ho.seq_historia = seq_historia), 'X', '')) as ingreso
      ,(SELECT IIF (EXISTS (select 1 from hc_historia_ocupacional where seq_eval = 2 and ho.seq_historia = seq_historia), 'X', '')) as periodico
      ,(SELECT IIF (EXISTS (select 1 from hc_historia_ocupacional where seq_eval = 3 and ho.seq_historia = seq_historia), 'X', '')) as egreso
      ,(SELECT IIF (EXISTS (select 1 from hc_historia_ocupacional where ho.otro_evaluacion != null and ho.seq_historia = seq_historia), ho.otro_evaluacion, '')) as otroCual
      ,(SELECT hc.nom_concepto from hc_concepto_historia hch inner join hc_concepto hc on hc.seq_concepto = hch.seq_concepto where ho.seq_historia = seq_historia) as concepto
FROM hc_personas p
inner join hc_historia_ocupacional ho on ho.seq_persona = p.seq_persona
inner join hc_historia_laboral lb on lb.seq_historia_laboral = ho.seq_historia_laboral
inner join hc_examen_fisico exa on exa.seq_exa_fisico = ho.seq_exa_fisico
inner join hc_aseguradora s on s.seq_aseguradora = p.seq_aseguradora
inner join hc_tipo_documento tipo on tipo.seq_tipo_documento = p.seq_tipo_documento
inner join hc_ciudad ciuR on ciuR.seq_ciudad = p.seq_lugar_residencia
inner join hc_ciudad ciuN on ciuN.seq_ciudad = p.seq_lugar_nacimiento
inner join hc_ciudad ciuO on ciuO.seq_ciudad = ho.seq_ciudad
where p.seq_persona = $P{numeroPersona}]]>
	</queryString>
