	<queryString>
		<![CDATA[SELECT seq_certificado
      ,recomendaciones
      ,dat_fecha_certificado
      ,nom_nombre
      ,num_numero
      ,tipo_restriccion_limitacion
      ,(SELECT IIF (EXISTS (select 1 from hc_certificado_gym where hc.seq_eval = '1'), 'X', '')) as apto
      ,(SELECT IIF (EXISTS (select 1 from hc_certificado_gym where hc.seq_eval = '2'), 'X', '')) as aptoR
      ,(SELECT IIF (EXISTS (select 1 from hc_certificado_gym where hc.seq_eval = '3'), 'X', '')) as noaptoC
      ,(SELECT IIF (EXISTS (select 1 from hc_certificado_gym where hc.seq_eval = '4'), 'X', '')) as noaptoLA
  FROM hc_certificado_gym hc
where hc.seq_certificado = $P{historia}]]>
